{
  "config": {
    "platform": {
      "php": "7.4.3"
    },
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "require": {
    "andreyryabin/sprint.migration": "4.2.1",
    "guzzlehttp/guzzle": "7.0",
    "ext-json": "*",
    "ext-curl": "*",
    "yngc0der/bitrix-rabbitmq": " v0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*",
    "phpmd/phpmd": "2.*"
  },
  "extra": {
    "installer-paths": {
      "local/modules/{$name}/": [
        "type:bitrix-d7-module",
        "type:bitrix-module"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Lepr\\": "local/lib/"
    }
  }
}
